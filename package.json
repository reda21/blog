{
    "private": true,
    "scripts": {
        "start": "npm run watch",
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "test": "cross-env NODE_ENV=test jest --watch",
        "test-js": "mocha-webpack --webpack-config=node_modules/laravel-mix/setup/webpack.config.js --require resources/js/test/setup.js resources/js/test/**/*.spec.js"
    },
    "jest": {
        "testRegex": "resources/js/test/.*.spec.js$",
        "moduleFileExtensions": [
            "js",
            "vue"
        ],
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/src/$1"
        },
        "transform": {
            "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.ts$": "<rootDir>/node_modules/babel-jest",
            ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
            ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
        },
        "snapshotSerializers": [
            "<rootDir>/node_modules/jest-serializer-vue"
        ]
    },
    "devDependencies": {
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@vue/test-utils": "^1.0.0-beta.33",
        "axios": "^0.19",
        "axios-mock-adapter": "^1.17.0",
        "babel-jest": "^25.1.0",
        "bootstrap": "^4.0.0",
        "cross-env": "^5.2.1",
        "jest": "^25.4.0",
        "jquery": "^3.5",
        "laravel-mix": "^5.0.1",
        "lodash": "^4.17.15",
        "popper.js": "^1.16.1",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.20.1",
        "sass-loader": "^8.0.0",
        "ts-jest": "^25.4.0",
        "vue": "^2.5.17",
        "vue-jest": "^3.0.5",
        "vue-template-compiler": "^2.6.10"
    },
    "dependencies": {
        "@babel/core": "^7.8.7",
        "@coreui/coreui": "3.0.0-beta.3",
        "@coreui/coreui-chartjs": "2.0.0-beta.0",
        "@coreui/icons": "1.0.0-beta.3",
        "@vue/composition-api": "^0.4.0",
        "@vue/server-test-utils": "^1.0.0-beta.31",
        "animated-vue": "^0.5.3",
        "babel-core": "^6.26.3",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-stage-2": "^6.24.1",
        "bootstrap-datepicker": "^1.9.0",
        "browser-sync": "^2.26.7",
        "browser-sync-webpack-plugin": "^2.2.2",
        "cropperjs": "^1.5.6",
        "favico.js": "^0.3.10",
        "font-awesome": "^4.7.0",
        "jest-serializer-vue": "^2.0.2",
        "jest-transform-stub": "^2.0.0",
        "jquery-confirm": "^3.3.4",
        "jquery-sortable": "^0.9.13",
        "jquery-ui": "^1.12.1",
        "jsdom": "^16.2.0",
        "jsdom-global": "^3.0.2",
        "jsonwebtoken": "^8.5.1",
        "mocha": "^7.0.1",
        "mocha-webpack": "^1.1.0",
        "mochapack": "^1.1.13",
        "moment": "^2.24.0",
        "pace-progress": "^1.0.2",
        "pc-bootstrap4-datetimepicker": "^4.17.50",
        "sinon": "^9.0.0",
        "toastr": "^2.1.4",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "unsplash-js": "^6.0.0",
        "vee-validate": "^3.2.5",
        "vue-bootstrap-datetimepicker": "^5.0.1",
        "vue-bootstrap-toasts": "^1.0.7",
        "vue-content-loading": "^1.6.0",
        "vue-image-crop-upload": "^2.5.0",
        "vue-notification": "^1.3.20",
        "vue-resource": "^1.5.1",
        "vue-router": "^3.1.5",
        "vue-simplemde": "^1.0.4",
        "vue-socket.io": "^3.0.7",
        "vuex": "^3.1.2",
        "webpack-bundle-analyzer": "^3.6.0",
        "ws": "^7.2.3"
    }
}
